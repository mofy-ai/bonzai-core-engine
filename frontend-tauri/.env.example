# 💜 MAMA BEAR'S FAMILY WORKSPACE ENVIRONMENT VARIABLES - EXAMPLE FILE! 🌟
# ================================================================
# Copy this file to .env and fill in your actual values!

# 🔑 Cloudflare Configuration
CLOUDFLARE_API_KEY=your_global_api_key_here
CLOUDFLARE_CA_KEY=your_ca_key_here

# 🌐 Cloudflare API Endpoints
CLOUDFLARE_API_BASE_URL=https://api.cloudflare.com/client/v4
CLOUDFLARE_ZONE_ID=your_zone_id_here
CLOUDFLARE_ACCOUNT_ID=your_account_id_here

# 🏠 Family AI Backend Configuration
FAMILY_BACKEND_URL=wss://mofy.ai/sse
MCP_ENDPOINT=https://mofy.ai/mcp
FAMILY_MEMORY_ID=your_mem0_id_here

# 🎯 Tauri Application Settings
TAURI_APP_NAME=Mama Bear Family IDE
TAURI_APP_VERSION=2.0.0
TAURI_BUNDLE_IDENTIFIER=ai.bonzai.mama-bear-ide

# 🔧 Development Settings
NODE_ENV=development
VITE_APP_TITLE=💜 Mama Bear's Family Home IDE
VITE_HOT_RELOAD=true

# 🎨 Theme Configuration
DEFAULT_THEME=purple-glassmorphism
ANIMATION_LEVEL=high
CELEBRATION_MODE=always

# 🚀 Performance Settings
ENABLE_SOURCE_MAPS=true
BUNDLE_ANALYZER=false
HOT_MODULE_RELOAD=true

# 📊 Monitoring & Analytics
ENABLE_FAMILY_MONITORING=true
LOG_LEVEL=info
DEBUG_MODE=false

